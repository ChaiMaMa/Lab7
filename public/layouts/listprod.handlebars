    <div class="my-list-prod">
        <form action="/listprod" method="get" style="padding-bottom: 10px;">
            <div class="row padMar">
                <div class="col padMar">
                    <div class="input-group">
                        <div class="input-group-prepend"></div>
                        <input class="form-control autocomplete" type="text" name="productName"
                            placeholder="Search your product">
                        <div class="input-group-append">
                            <button class="btn btn-warning" type="submit"
                                style="background-color:#828887; color: white">
                                <i class="fa fa-search"></i>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
            <p style="padding-top: 10px;">(Leave blank for all products)</p>
        </form>
        <h1>All Products</h1>
        <div data-reflow-type="product-list" data-reflow-order="name_asc" data-reflow-layout="cards">
